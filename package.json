{
  "name": "@everglot/everglot",
  "description": "Everglot web application",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "run-p watch:*",
    "watch:css": "postcss src/assets/global.pcss -o static/global.css -w",
    "watch:dev": "dotenv sapper dev",
    "build": "run-s build:css build:css:fonts build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.pcss -o static/global.css",
    "build:css:fonts": "run-p build:css:fonts:poppins build:css:fonts:noto build:css:fonts:notosc build:css:fonts:notojp",
    "build:css:fonts:poppins": "NODE_ENV=production postcss src/assets/fonts/poppins.pcss -o static/fonts/poppins.css",
    "build:css:fonts:noto": "NODE_ENV=production postcss src/assets/fonts/noto-sans.pcss -o static/fonts/noto-sans.css",
    "build:css:fonts:notosc": "NODE_ENV=production postcss src/assets/fonts/noto-sans-sc.pcss -o static/fonts/noto-sans-sc.css",
    "build:css:fonts:notojp": "NODE_ENV=production postcss src/assets/fonts/noto-sans-jp.pcss -o static/fonts/noto-sans-jp.css",
    "build:sapper": "sapper build --legacy",
    "build:export": "sapper export --legacy",
    "export": "run-s build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "validate": "svelte-check --ignore src/node_modules/@sapper --compiler-warnings 'module-script-reactive-declaration:ignore'",
    "migrate": "node-pg-migrate -m src/migrations",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "jest --forceExit --runInBand",
    "test:pretty": "jest --forceExit --runInBand | roarr pretty-print",
    "test:watch": "jest --watch --forceExit --runInBand",
    "test:watch:pretty": "jest --watch --forceExit --runInBand | roarr pretty-print",
    "lint": "eslint src",
    "generate-invite-token": "dotenv ts-node -- scripts/generateInviteToken.ts"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@fluent/bundle": "^0.17.0",
    "@graphile-contrib/pg-many-to-many": "^1.0.1",
