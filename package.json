{
  "name": "@everglot/everglot",
  "description": "Everglot web application",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "run-p watch:*",
    "watch:css": "postcss src/assets/global.pcss -o static/global.css -w",
    "watch:dev": "sapper dev",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.pcss -o static/global.css",
    "build:sapper": "sapper build --legacy",
    "build:export": "sapper export --legacy",
    "export": "run-s build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "validate": "svelte-check --ignore src/node_modules/@sapper --compiler-warnings 'module-script-reactive-declaration:ignore'",
    "migrate": "node-pg-migrate -m src/migrations",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "jest --forceExit --runInBand",
    "test:pretty": "jest --forceExit --runInBand | roarr pretty-print",
    "test:watch": "jest --watch --forceExit --runInBand",
    "test:watch:pretty": "jest --watch --forceExit --runInBand | roarr pretty-print",
    "lint": "eslint src"
  },
  "engines": {
    "node": ">=12.0.0",
