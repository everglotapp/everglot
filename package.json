    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "firebase-admin": "^10.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "google-auth-library": "^7.10.2",
    "graphql": "^15.6.1",
